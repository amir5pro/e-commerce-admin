
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}


model Store {
  id     String     @id @default(uuid())
  name   String
  userId String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}